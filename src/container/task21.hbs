<div>

  <section class="card card-body">
    {{#with heading}}
    {{#with main}}
    <div class="card card-body">
      <div class="display-1">{{title}}</div>
      <br>
      <p>{{text}}</p>
      <br>
      <div>
        <button class="btn btn-primary">{{button_text.solid}}</button>
      </div>
      <br>
      <div>
        <button class="btn btn-outline-primary">{{button_text.outline}}</button>
      </div>
    </div>
    {{/with}}

    <br>

    {{#with second}}
    <div class="card card-body">
      <div class="display-2">{{title}}</div>
      <br>
      <p>{{text}}</p>
      <br>
      <div>
        <button class="btn btn-secondary">{{button_text.solid}}</button>
      </div>
      <br>
      <div>
        <button class="btn btn-outline-secondary">{{button_text.outline}}</button>
      </div>
    </div>
    {{/with}}
    <br>
    {{#with subblock}}
    <div class="card card-body">
      <div class="display-3">{{title}}</div>
      <br>
      <p>{{text}}</p>
      <br>
      <div>
        <button class="btn btn-dark">{{button_text.solid}}</button>
      </div>
      <br>
      <div>
        <button class="btn btn-outline-dark">{{button_text.outline}}</button>
      </div>
    </div>
    {{/with}}

    {{/with}}
  </section>
  <br>
  <section class="card card-body">

    {{#with nested_display}}
    <div class="display-1">{{title}}</div>
    <br>
    {{#with children_display}}
    <div class="card card-body ">

      <div class="display-4">{{title}}</div>
      <br>
      <p>{{text}}</p>
      <div>
        <button class="btn btn-outline-warning">{{button.text}}</button>

      </div>
    </div>
    {{/with}}
    <br>
    <div class="display-6">{{header_text}}</div>
    <br>
    <div>
      <button class="btn btn-info text-light"> {{button.text}}</button>
    </div>
    {{/with}}

  </section>
  <br>
  <section class="card card-body ">
    {{#with paragraph }}
    <p class="text-uppercase">{{title}}</p>
    <br>
    <p class="text-lowercase">{{text}}</p>
    <br>
    <div>
      <a href="{{button.url}}" class="btn btn-link">{{button.text}}</a>
    </div>
   
    <div class="card card-body ">
      <nav>
        <ul>
        {{#each list_name}}
        <li><a href="{{url}}">{{text}}</a></li>
        <br>
 {{/each}}
 </ul>
      </nav>

    </div>
     {{/with}}
  </section>
</div>